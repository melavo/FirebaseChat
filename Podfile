  pod 'FirebaseAnalytics'
  pod 'FirebaseMessaging'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  pod 'Kingfisher'
  pod 'Firebase/Firestore'
  pod 'ALLoadingView'

end
